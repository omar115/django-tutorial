{
  "version": 2,
  "builds": [
    {
      "src": "mysite/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
    },
    {
    "src": "build.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles_build/static"
    }
  }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "mysite/wsgi.py"
    }
  ]
}
